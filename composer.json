{
    "name": "nodrew/comment-bundle",
    "type": "symfony-bundle",
    "description": "This Bundle provides integration with the Google DFP Ad Network for Symfony2 applications",
    "keywords": ["dfp", "advertising", "google"],
    "homepage": "https://github.com/dbtlr/NodrewDfpBundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Drew Butler",
            "email": "hi@dbtlr.com"
        }
    ],
    "support": {
        "issues": "https://github.com/dbtlr/NodrewDfpBundle/issues"
    },
    "require": {
        "php": ">=5.3.2",
        "symfony/symfony": ">=2.0,<2.4-dev"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload": {
        "psr-0": { "Nodrew\\Bundle\\DfpBundle": "" }
    },
    "target-dir": "Nodrew/Bundle/DfpBundle"
}
